{
  "name": "chessCoordinatesTrainer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/chessCoordinatesTrainer/src",
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/chessCoordinatesTrainer",
        "browser": "apps/chessCoordinatesTrainer/src/main.ts",
        "polyfills": ["zone.js", "apps/chessCoordinatesTrainer/src/polyfills.ts"],
        "tsConfig": "apps/chessCoordinatesTrainer/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/chessCoordinatesTrainer/src/assets",
            "output": "assets"
          }
        ],
        "styles": [
          "apps/chessCoordinatesTrainer/src/global.scss",
          "apps/chessCoordinatesTrainer/src/theme/variables.scss",
          "apps/chessCoordinatesTrainer/src/theme/board.scss"
        ],
        "index": "apps/chessCoordinatesTrainer/src/index.html"
      },
      "configurations": {
        "production": {
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "apps/chessCoordinatesTrainer/src/environments/environment.ts",
              "with": "apps/chessCoordinatesTrainer/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "chessCoordinatesTrainer:build:production"
        },
        "development": {
          "buildTarget": "chessCoordinatesTrainer:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/chessCoordinatesTrainer/jest.config.ts"
      }
    }
  }
}